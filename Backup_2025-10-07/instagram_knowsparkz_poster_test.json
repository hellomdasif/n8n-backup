      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        300,
        200
      ],
      "parameters": {
        "language": "javaScript",
        "jsCode": "// KnowSparkz account configuration\nconst config = {\n  \"account_name\": \"knowsparkz_test\",\n  \"node_id\": \"17841454629202811\",\n  \"credential_id\": \"XkFi9HSVhe9e5VV8\",\n  \"data_table_id\": \"YpfR5PAwNwDkmpbe\",\n  \"status_column\": \"ks_insta_status\",\n  \"video_base_url\": \"https://files.mohdasif.net/knowsparkz/\",\n  \"processing_required\": true,\n  \"processing_config\": {\n    \"overlay_script\": \"/files/sh/overlay_with_views.sh\"\n  },\n  \"hashtags\": \"#knowledge #education #learning #growth #knowsparkz #motivation #success #tips #wisdom #inspiration\"\n};\n\nreturn { config };"
      }
    },
    {
      "id": "get-content",
      "name": "Get Content from Data Table",
      "type": "n8n-nodes-base.n8nTrainingCustomerDatastore",
      "typeVersion": 1,
      "position": [
        500,
        200
      ],
      "parameters": {
        "operation": "getAll",
        "datastore": "={{$node['Get Config'].json.config.data_table_id}}",
        "options": {
          "where": {
            "conditions": [
              {
                "key": "={{$node['Get Config'].json.config.status_column}}",
                "value": "pending"
              }
            ]
          },
          "limit": 1,
          "sort": {
            "field": "id",
            "direction": "ASC"
          }
        }
      }
    },
    {
      "id": "check-content",
      "name": "Check if Content Available",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [
        700,
        200
      ],
      "parameters": {
        "conditions": {
          "options": {
            "leftValue": "",
            "caseSensitive": true,
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "content-exists",
              "leftValue": "={{$json.length}}",
              "rightValue": 0,
              "operator": {
                "type": "number",
                "operation": "gt"
              }
            }
          ],
          "combinator": "and"
        }
      }
    },
    {
      "id": "no-content-stop",
      "name": "No Content - Stop",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [
        900,
        100
      ],
      "parameters": {}
    },
    {
      "id": "extract-content",
      "name": "Extract Content Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        900,
        300
      ],
      "parameters": {
        "language": "javaScript",
        "jsCode": "const contentItem = $input.all()[0].json[0];\nconst config = $('Get Config').first().json.config;\n\n// Build video URL\nconst videoUrl = config.video_base_url + contentItem.filename;\n\n// Build caption with hashtags\nconst caption = `${contentItem.caption}\\n\\n${config.hashtags}`;\n\nreturn {\n  content_id: contentItem.id,\n  video_url: videoUrl,\n  caption: caption,\n  config: config\n};"
      }
    },
    {
      "id": "video-processing",
      "name": "Video Processing Module",
      "type": "n8n-nodes-base.executeWorkflow",
      "typeVersion": 1,
