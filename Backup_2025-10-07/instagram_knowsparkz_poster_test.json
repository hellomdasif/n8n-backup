        "waitForSubWorkflow": true,
        "source": "database"
      }
    },
    {
      "id": "update-status",
      "name": "Update Status in Data Table",
      "type": "n8n-nodes-base.n8nTrainingCustomerDatastore",
      "typeVersion": 1,
      "position": [
        1500,
        300
      ],
      "parameters": {
        "operation": "update",
        "datastore": "={{$node['Extract Content Data'].json.config.data_table_id}}",
        "keyName": "id",
        "keyValue": "={{$node['Extract Content Data'].json.content_id}}",
        "fieldsToSet": {
          "fields": [
            {
              "fieldName": "={{$node['Extract Content Data'].json.config.status_column}}",
              "fieldValue": "={{$node['Instagram Posting Module'].json.status}}"
            },
            {
              "fieldName": "knowsparkz_post_id",
              "fieldValue": "={{$node['Instagram Posting Module'].json.post_id || 'N/A'}}"
            },
            {
              "fieldName": "knowsparkz_processed_at",
              "fieldValue": "={{new Date().toISOString()}}"
            },
            {
              "fieldName": "knowsparkz_error",
              "fieldValue": "={{$node['Instagram Posting Module'].json.error || ''}}"
            }
          ]
        }
      }
    }
